<section class="top-links">
  <div class="row">
    <div class="external-links small-12 medium-9">
      <ul class="menu">
        <%= render "shared/admin_login_items" %>
        <li>
          <%= link_to faq_path do %>
            <span class="icon-comments"></span>
            <%= t("layouts.footer.faq") %>
          <% end %>
        </li>
        <li>
          <%= link_to t("layouts.header.external_link_institution_url") do %>
            <%= image_tag("CabildoGranCanaria.jpg") %>
            <%# <%= t("layouts.header.external_link_institution") %>
          <% end %>
        </li>
        <% if setting['blog_url'] %>
          <li>
            <%= link_to setting['blog_url'], title: t('shared.target_blank_html'), target: "_blank" do %>
              <%= t("layouts.header.external_link_blog") %>
            <% end %>
        </li>
        <% end %>
      </ul>
      <ul class="menu">
        <%= render "devise/menu/login_items" %>
      </ul>

   </div>
  </div>
</section>
